{
  "kind": "collectionType",
  "collectionName": "faq_qas",
  "info": {
    "singularName": "faq-qa",
    "pluralName": "faq-qas",
    "displayName": "FAQ Q&A",
    "description": "Frequently Asked Questions for stores - can be global or store-specific"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "question": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 500,
      "required": true
    },
    "answer": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "question",
      "required": false
    },
    "is_global": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "stores": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store.store",
      "inversedBy": "faq_qas"
    },
    "category": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 100
    },
    "order": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "is_active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "published_date": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "end_date": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
