{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": "Cannabis products from POS/menu systems"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sku": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 100
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category"
    },
    "price": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "thc_content": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 50
    },
    "cbd_content": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 50
    },
    "product_type": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "enum": [
        "flower",
        "vaporizer",
        "edible",
        "concentrate",
        "topical",
        "tincture",
        "pre-roll",
        "accessory",
        "other"
      ]
    },
    "is_active": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": true
    },
    "pos_product_id": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 100
    },
    "dutchie_product_id": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 100
    }
  }
}
