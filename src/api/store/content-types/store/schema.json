{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store",
    "description": "Physical dispensary store locations"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 200,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "store_code": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 50,
      "unique": true
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand"
    },
    "region": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::region.region",
      "inversedBy": "stores"
    },
    "address": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "common.address",
      "repeatable": false,
      "required": true
    },
    "geo": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "phone": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 20
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "timezone": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "America/New_York"
    },
    "services": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "store.service-tag",
      "repeatable": true
    },
    "amenities": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "store.amenity-tag",
      "repeatable": true
    },
    "is_24hours": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "hours": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "common.hours",
      "repeatable": true
    },
    "hour_exceptions": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "common.hours-exception",
      "repeatable": true
    },
    "hero_media": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "gallery": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "cta_blocks": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "ui.cta",
      "repeatable": true
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "seo.meta",
      "repeatable": false
    },
    "is_active": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": true
    },
    "pos_location_id": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "maxLength": 100
    },
    "menu_url": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "online_ordering_url": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "promotions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::promotion.promotion",
      "mappedBy": "store"
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "stores"
    },
    "announcements": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::announcement.announcement",
      "mappedBy": "stores"
    },
    "LicenseNumber": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isMedical": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isRecreational": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "hasCafe": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "DutchieStoreID": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "multi_location_selector": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "offers": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "store.offer",
      "repeatable": true
    },
    "dutchieApiKey": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "google_place_id": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "summary": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": "Mint Cannabis is your trusted dispensary for cannabis products and a welcoming, community-focused experience. Whether you're a medical patient or an adult-use customer, our knowledgeable budtenders are here to guide you, answer questions, and help you discover the perfect products for your needs. At every Mint location, you'll find quality, value, and the exceptional service that defines the Mint experience. "
    },
    "faq_qas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::faq-qa.faq-qa",
      "mappedBy": "stores"
    },
    "faqqas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::faqqa.faqqa",
      "mappedBy": "stores"
    },
    "about": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "discription": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "google_map_link": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "google_map_embed": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
